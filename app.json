{
    "expo": {
        "name": "Instagram test clone",
        "slug": "rn-inst-m",
        "platforms": [
            "ios",
            "android"
        ],
        "version": "1.2.1",
        "orientation": "default",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.mi-q87-dev.inst-m-rn-app",
            "buildNumber": "1.2.1"
        },
        "android": {
            "package": "com.mi_q87_dev.inst_m_rn_app",
            "versionCode": 4,
            "permissions": [
                "CAMERA"
            ]
        }
    }
}
